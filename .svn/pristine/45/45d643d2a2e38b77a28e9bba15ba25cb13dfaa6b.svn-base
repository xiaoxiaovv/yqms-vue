.dialog_box {
    position: fixed;
    left: 0;
    top: 0;
    height: 100%;
    width: 100%;
    background: rgba(0, 0, 0, 0.5);
    z-index: 1200;
    text-align: center;
}
.dialog_box::before {
    content: "";
    height: 100%;
    display: inline-block;
    vertical-align: middle;
}
.dialog_content {
	min-width: 280px;
    min-height: 200px;
    background: #ffffff;
    border-radius: 5px;
    position: static;
    top: auto;
    left: auto;
    transform: none;
    margin-left: 0;
    display: inline-block;
    vertical-align: middle;
}
.dialog_title_box {
	width: 100%;
	height: auto;
    position: relative;
}
.dialog_title {
    height: 70px;
    line-height: 70px;
    font-size: 20px;
    font-weight:bold;
    color: #333;
    text-align: center;
    box-sizing: border-box;
    border-bottom: 1px solid #e5e5e5;
    position: relative;
}
.dialog_close {
	position: absolute;
    right: 10px;
    top: 10px;
    font-size: 28px;
    color: #acacac;
    width: 14px;
    height: 14px;
    line-height: 14px;
    text-align: center;
    cursor: pointer;
}
.content {
 	color: #333;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}
.btns_box {
	text-align: center;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}
.btn {
	display: inline-block;
	width: 120px;
    height: 36px;
    line-height: 34px;
    color: #333;
    font-size: 14px;
    text-align: center;
    background: #fff;
    border-radius: 18px;
    cursor: pointer;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    margin: 18px 0;
}
.save_btn {
	color: #fff;
    background: #44b0e6;
    border: 1px solid #44b0e6;
}
.cancle_btn {
	color: #333;
    background: #fff;
    border: 1px solid #ccc;
    margin-left: 20px;
}

.cancle_success_box .dialog_content .dialog_close {
    display: none;
}