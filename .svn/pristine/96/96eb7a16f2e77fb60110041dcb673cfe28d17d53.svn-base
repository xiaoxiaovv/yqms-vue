.commom_dialog_box {
	position: fixed;
    left: 0;
    top: 0;
    height: 100%;
    width: 100%;
    background: rgba(0, 0, 0, 0.5);
    z-index: 1200;
    text-align: center;
}

.commom_dialog_box::before {
    content: "";
    height: 100%;
    display: inline-block;
    vertical-align: middle;
}
.batch_Infoexport_wary {
    width: 310px;
    height: 180px;
    position: absolute;
    background-color: #fff;
    border-radius: 5px;
    top: 0px;
    left: 0px;
    z-index: 200;
  }
  
  .batch_Infoexport_wary .batch_header {
    font-family: 'STHeitiSC-Medium';
    width: 100%;
    height: 45px;
    line-height: 45px;
    border-bottom: 1px solid #E6E6E6;
    font-size: 16px;
    color: #333333;
    padding: 0 29px;
    box-sizing: border-box;
  }
  .batch_Infoexport_wary .batch_header .dialog_message {
    font-weight: 700;
  }
  
  .batch_Infoexport_wary .dialog_mask_close {
    position: absolute;
    right: 20px;
    top: 10px;
    width: 25px;
    height: 25px;
    line-height: 25px;
    color: #acacac;
    font-size: 24px;
    z-index: 2;
    cursor: pointer;
  }
  
  .batch_Infoexport_wary .contentBtn {
    margin-top: 50px;
    padding: 0 40px;
  
  }
  .batch_Infoexport_wary .contentBtn button {
    background-color: #fff;
    font-size: 14px;
    color: #333;
    border: 1px solid #333;
    border-radius: 6px;
    padding: 5px;
    box-sizing: border-box;
  }
  .batch_Infoexport_wary .contentBtn button:hover {
    cursor: pointer;
  }
  .batch_Infoexport_wary .contentBtn .checkBtn {
    color: #00B2E6;
    border: 1px solid #00B2E6;
  }
  
  .batch_Infoexport_wary .contentBtn .exportBtn {
    margin-left: 16px;
  }
  
  .success_wary {
    width: 440px;
    height: 200px;
    border-radius: 6px;
    text-align: center;
    padding: 0 30px;
    font-family: STHeitiSC-Medium;
    left: 50%;
    top: 50%;
    margin-left: -220px;
    margin-top: -100px;
  }
  
  .success_wary p {
    width: 440px;
    font-size: 16px;
    margin: 0 auto;
    margin-top: 12px;
    line-height: 30px;
    color: #333;
    font-weight: 500;
  }
  .success_wary p>i {
    width: 23px;
    height: 23px;
    display: inline-block;
    vertical-align: middle;
    background: url('../../assets/images/home/classic/check.svg') no-repeat;
  }
  
  .success_wary .gotoBtn a {
    display: inline-block;
    width: 120px;
    height: 36px;
    line-height: 36px;
    text-align: center;
    border-radius: 18px;
    font-size: 14px;
    color: #333;
    border: 1px solid #ccc;
    box-sizing: border-box;
    background: #fff;
    margin-top: 30px;
  }
  .success_wary .gotoBtn .gotoCheck {
    background-color: #00B2E6;
    border: none;
    color: #fff;
  }
  .success_wary .gotoBtn .gotoCheck:hover {
    cursor: pointer;
  }
  .success_wary .gotoBtn .laterCheck {
    margin-left: 20px;
  }
  .success_wary .gotoBtn .laterCheck:hover {
    cursor: pointer;
  }