.footer{
	font-size: 12px;
	background: var(--bg-footer);
	width: 100%;
	height: 105px;
	padding: 0 75px 0 90px;
	box-sizing: border-box;
	color: var(--c-e9);
}

.footer_l p{
	width: 430px;
	white-space: nowrap;
	word-break: break-word;
	text-overflow: ellipsis;
	overflow: hidden;
	margin: 5px 0;
}
.footer_r p {
	width: 275px;
	white-space: nowrap;
	word-break: break-word;
	text-overflow: ellipsis;
	overflow: hidden;
}
.foot_img {
	width: 65px;
    height: 54px;
	background: var(--bg-head-icon) 0 -44px;
}
.foot_icon_box {
	height: 25px;
}
.defined_foot_icon {
	float: right;
	margin-right: 5px;
    width: 25px;
    height: 25px;
    background-position: -176px 0;
    cursor: pointer;
    opacity: 0;
    background: var(--bg-head-foot-icon) no-repeat -176px 0;
}
.footer:hover .defined_foot_icon {
  opacity: 1;
}
.foot_right {
	margin-top: 3px;
}
/* muya  start */
.footer_isentia {
	border-top: 1px solid #ccc;
}
.foot_img_gray {
	background-position: -72px -49px;
	margin-top: 53px;
}
.defined_foot_icon_isentia{
	background-position: -105px -5px;
}
/* muya  end */

/* lmy start */
.footer_img_green {
	background: var(--bg-footer_img_green);
	width: 22px;
    height: 22px;
    position: relative;
	display: inline-block;
	vertical-align: middle;
}
.footer_green .footer_copy_green {
	display: inline;
}
.footer_green .footer_copy_green a {
	font-family: "ו";
}
.item_vertical_green {
	width: 350px;
	text-align: center;
}
.item_vertical_green .company_green {
	font-family: "ו";
}
.item_vertical_green .nsfocus a{
	color: var(--c-white);
	font-family: "ו";
}
/* lmy end */
.footer_unicom {
	height: 150px;
	padding: 0;
}
.footer_unicom .flex_between{
	width: 70%;
    height: 150px;
    background-color: var(--bg-footer);
    color: var(--bg-rgb65);
    margin: 0 auto;
    padding: 0 15%;
    overflow: hidden;
}
.footer_unicom .flex_between .footer_content {
	border-top: 2px solid var(--c-414447);
    border-bottom: 2px solid var(--c-414447);
    line-height: 48px;
    height: 48px;
    margin-top: 33px;
    color: var(--bg-rgb65);
	font-size: 12px;
	width: 100%;
}
.footer_unicom .flex_between .footer_content p {
	float: left;
    color: var(--c-d7d7d6);
}
.footer_unicom .flex_between .footer_content p a {
	display: inline-block;
    padding-right: 40px;
    color: var(--bg-rgb65);
    font-size: 12px;
}
.footer_unicom .flex_between .footer_content .img_wrap {
	float: right;
}
.footer_unicom .flex_between .footer_content .img_wrap .img {
	float: left;
    width: 29px;
    height: 29px;
    background: var(--bg-head-icons) 0 -70px no-repeat;
    margin-top: 9px;
}