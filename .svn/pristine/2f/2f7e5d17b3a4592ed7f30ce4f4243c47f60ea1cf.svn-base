.iconbg_01 {
	width: 18px;
	height: 24px;
	margin-right: 6px;
	background-position: -9px 0;
	vertical-align: middle;
}

.icon_center_push {
	position: relative;
	top: 3px;
	display: inline-block;
	width: 16px;
	height: 16px;
	margin-right: 2px;
	background: var(--bg-centericons) no-repeat;
}

.icon_circle_box {
	position: relative;
	top: 27px;
	left: 10px;
	border-right: 1px solid var(--c-e);
	z-index: 88;
}

.icon_circle {
	position: absolute;
	top: -18px;
	left: -24px;
	margin: 8px 0 0 16px;
	background-position: -37px -75px;
	z-index: 99999;
}

.icon_circle_active {
	background-position: -10px -74px;
}

.push_box {
	margin: 3px 0;
	height: 499px;
	width: 100%;
	overflow: hidden;
	box-sizing: border-box;
	padding: var(--home-push-padding);
}

.dashed_bottom {
	padding: 8px 0 4px;
	border-bottom:1px dashed var(--c-f1);
}

.push_item:hover .icon_circle {
	background-position: -10px -74px;
}

.push_item:hover .push_info_from {
	color: var(--c-white);
}
.push_item:last-child .dashed_bottom{
	border-bottom: none;
}
.push_info {
	font-size: 15px;
	height: 80px;
	padding: 5px 13px 5px;
	margin: 10px 18px 0 20px;
	box-sizing: border-box;
	width: 100%;
	overflow: hidden;
}

.push_info_first {
	margin-top: 0;
}

.push_info:hover,
.push_info_active {
	background: linear-gradient(90deg, rgba(255, 185, 150, 1), rgba(251, 126, 145, 1));
	border-radius: 3px;
	color: var(--c-white);
	z-index: 111;
}

.push_info .push_info_title {
	letter-spacing: 1px;
	font-size: 15px;
	height: 44px;
	line-height: 22px;
	color: var(--c-23);
	overflow: hidden;
	-webkit-line-clamp: 2;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	word-wrap:break-word;
	word-break:break-all;
}
.push_info .push_title {
	display: block;
	color: var(--c-23);
	letter-spacing: 1px;
}
.push_info_active .push_title, 
.push_info_active:hover .push_title, 
.push_info:hover .push_title  {
	color: var(--c-white);
}
.push_info .push_info_from {
	margin-top: 6px;
	font-size: 12px;
	color: var(--c-8);
}

.push_info .push_info_from_active {
	color: var(--c-white);
}

.icon_border_unicom {
	background: none;
	display: inline-block;
    width: 6px;
    height: 16px;
    margin-right: 5px;
    background: var(--border-5b9fea);
    vertical-align: top;
    margin-top: 3px;
	margin-left: 10px;
}
.model_content_box_unicom {
	background: var(--bg-rgb2556);
}
