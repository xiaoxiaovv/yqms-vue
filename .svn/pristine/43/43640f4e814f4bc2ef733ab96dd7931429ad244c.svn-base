.wrap {
    background-color: var(--bg-wrap);
}
.judgment_center {
	background: var(--bg-content);
}
.apply_wait_box {
    text-align: center;
    font-size: 16px;
    padding-top: 180px;
    background: #fff;
    width: 80%;
    min-width: 1200px;
    max-width: 1600px;
    margin: var(--judgment-model-margin);
    min-height: var(--judgment_model-height);
}
.img_box {
    width: 147px;
    height: 119px;
    margin: 0 auto 40px;
}
.img_box img {
    display: block;
    width: 147px;
    height: 119px;
}
.wait_text {
    color: #5090F1;
    margin-bottom: 20px;
}
.wait_tel {
    color: #333;
}
