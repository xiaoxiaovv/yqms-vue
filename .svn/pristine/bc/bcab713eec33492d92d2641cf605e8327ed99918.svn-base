.masker {
	width: 100%;
	height:100%;
	background: var(--bg-rgb4);
	margin: var(--source-model-margin);
    min-height: 40vh;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
}
.judgment_nodata {
	width: 100%;
	color: var(--c-3);
	font-size: 14px;
	text-align: center;
	min-height: 60vh;
}