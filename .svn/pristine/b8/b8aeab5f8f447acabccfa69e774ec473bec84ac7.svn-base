.icon_file {
	display: inline-block;
	width: 16px;
	height: 16px;
	background: var(--bg-centericons) no-repeat;
	background-position: -154px -152px;
	position: relative;
	top: 3px;
	margin-right: 3px;
}

.el-tree-label {
	font-size: 14px;
	color: var(--c-46);
	height: 30px;
	line-height: 30px;
	margin: 10px 0;
}

.el-tree-node__content {
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    height: 47px;
    cursor: pointer;
}